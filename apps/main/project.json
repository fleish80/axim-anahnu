{
  "name": "main",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "aa",
  "sourceRoot": "apps/main/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/main/browser",
        "index": "apps/main/src/index.html",
        "main": "apps/main/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/main/tsconfig.app.json",
        "assets": [
          "apps/main/src/favicon.ico",
          "apps/main/src/assets",
          {
            "glob": "**/*",
            "input": "./i18n/",
            "output": "./i18n/"
          }
        ],
        "styles": ["apps/main/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/main/src/environments/environment.ts",
              "with": "apps/main/src/environments/environment.development.ts"
            }
          ]
        },
        "firebase": {
          "fileReplacements": [
            {
              "replace": "apps/main/src/environments/environment.ts",
              "with": "apps/main/src/environments/environment.firebase.ts"
            }
          ],
          "optimization": true,
          "namedChunks": false,
          "aot": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "main:build:production"
        },
        "development": {
          "browserTarget": "main:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "main:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/main/**/*.ts", "apps/main/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/main/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/main/server",
        "main": "apps/main/server.ts",
        "tsConfig": "apps/main/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true,
          "fileReplacements": [
            {
              "replace": "apps/main/src/environments/environment.ts",
              "with": "apps/main/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "main:build:development",
          "serverTarget": "main:server:development"
        },
        "production": {
          "browserTarget": "main:build:production",
          "serverTarget": "main:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "production": {
          "browserTarget": "main:build:production",
          "serverTarget": "main:server:production"
        },
        "development": {
          "browserTarget": "main:build:development",
          "serverTarget": "main:server:development"
        }
      },
      "defaultConfiguration": "production"
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {
        "version": 2,
        "browserTarget": "main:build:production",
        "serverTarget": "main:server:production",
        "prerenderTarget": "main:prerender:production"
      }
    }
  }
}
